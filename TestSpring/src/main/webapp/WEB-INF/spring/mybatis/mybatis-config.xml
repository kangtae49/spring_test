<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "HTTP://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
    <settings>
        <setting name="cacheEnabled" value="true" />
        <setting name="lazyLoadingEnabled" value="true" />
        <setting name="multipleResultSetsEnabled" value="true" />
        <setting name="useColumnLabel" value="true" />
        <setting name="useGeneratedKeys" value="false" />
        <!-- <setting name="enhancementEnabled" value="false" /> -->
        <setting name="defaultExecutorType" value="SIMPLE" />
        <!-- <setting name="defaultStatementTimeout" value="25000" /> -->
        <setting name="defaultListResultHandlerType" value="java.util.ArrayList" />
        <setting name="defaultMapResultHandlerType" value="java.util.HashMap" />
        <setting name="defaultMapResultHandlerType" value="java.util.HashMap" />
        
        <setting name="defaultAutoCommit" value="true" />
        
        <!-- 
        <setting name="validationQuery" value="select 1 from TB_VSFS_TABLE_QUERY_INFO limit 1;" />
        <setting name="testWhileIdle" value="true"/>
        <setting name="timeBetweenEvictionRunsMillis" value="10000"/>
         -->
    </settings>
</configuration>